# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Test-inke' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks 使用时出现问题http://blog.csdn.net/feng2qing/article/details/73835805
use_frameworks!

  # Pods for Test-inke
  pod 'Socket.IO-Client-Swift', '~> 8.3.3' #这里有个问题，最新版本的socketio，连接不到我的后台node服务器，但是回退到这个版本就能调用了
pod 'AFNetworking', '~> 3.1.0'
pod 'SDWebImage', '~> 4.0.0'
pod 'Masonry', '~> 1.0.2'
pod 'YYKit', '~> 1.0.9'
pod 'MJExtension'
pod 'MJRefresh'
pod 'LFLiveKit'
pod 'UMengUShare/UI'
# 集成新浪微博 【友盟官网技术文档这里标点符号有错，导入的时候记得更改】
pod 'UMengUShare/Social/Sina'
# 集成微信
pod 'UMengUShare/Social/WeChat'
# 集成QQ
pod 'UMengUShare/Social/QQ'
